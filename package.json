{
  "name": "@becomes/cms-starter-blog",
  "version": "1.0.0",
  "description": "Starter project which initializes BCMS with example data for a typical blog website.",
  "main": "index.js",
  "scripts": {
    "start": "bcms",
    "start:backend": "bcms --backend",
    "dev": "bcms --dev",
    "dev:ui": "bcms --dev --ui",
    "dev:backend": "bcms --dev --backend",
    "plugin:build": "npm run lint:plugins && tsc -p tsconfig.backend.json",
    "src:build": "npm run lint:src && tsc -p tsconfig.src.json",
    "ui:build": "bcms --ui",
    "lint:plugins": "eslint plugins --ext .ts",
    "lint:src": "eslint src --ext .ts"
  },
  "author": "Becomes",
  "dependencies": {
    "@becomes/cms-backend": "^2.0.39",
    "@becomes/cms-bundler": "^2.0.5",
    "@becomes/cms-ui": "^2.0.43"
  },
  "devDependencies": {
    "@becomes/cms-sdk": "^2.0.11",
    "@tsconfig/svelte": "^1.0.0",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "node-sass": "^4.14.1",
    "svelte": "^3.31.2",
    "svelte-check": "^1.1.26",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becomesco/bcms-starter-blog.git"
  },
  "bugs": {
    "url": "https://github.com/becomesco/bcms-starter-blog/issues"
  },
  "homepage": "https://github.com/becomesco/bcms-starter-blog#readme"
}
